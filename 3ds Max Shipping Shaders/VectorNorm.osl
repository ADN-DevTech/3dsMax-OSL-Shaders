shader VectorNorm
[[ string help     = "Normalize a Vector",
   string label    = "Normalize (vector)",
   string category = "Math\Vector" ]]
(
	vector Input = 0.0,
	output vector Out = 0.0,
)
{
	Out = normalize(Input);
}
