shader FloatATan
[[ string help = "Arc Tangent (in radians) of a float",
   string label= "ATan",
   string category = "Math\Float" ]]
(
	float Input = 0.0,
	output float Out = 0.0 [[ string label="Angle (radians)" ]],
)
{
	Out = atan(Input);
}
