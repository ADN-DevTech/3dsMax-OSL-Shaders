shader FloatMod
[[ string help = "Modulo (remainder of division) between two floats",
   string label= "Modulo",
   string category = "Math Float" ]]
(
	float A = 0.0,
	float B = 1.0,
	output float Out = 0.0,
)
{
	Out = fmod(A, B);
}
